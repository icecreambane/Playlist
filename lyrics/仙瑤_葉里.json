[
  {"time": "00:24.11", "line": "眉眼里藏的笑 如星落九霄"},
  {"time": "00:29.88", "line": "纤姿舞的曼妙 惹月色倾倒"},
  {"time": "00:35.40", "line": "嫣然笑 几分娇"},
  {"time": "00:37.87", "line": "不染风尘铸的刀"},
  {"time": "00:41.04", "line": "悬琴箫 风雅温柔声色告"},
  {"time": "00:47.32", "line": "娇容调的色调 惊艳的刚好"},
  {"time": "00:53.04", "line": "梨涡晕的正俏 潋滟如云渺"},
  {"time": "00:58.47", "line": "紫衫飘 蒲扇摇"},
  {"time": "01:00.89", "line": "无须一分多与少"},
  {"time": "01:03.76", "line": "墨色烧 釉彩膏泥光色烙"},
  {"time": "01:09.26", "line": "你似携月而来的仙瑶"},
  {"time": "01:15.02", "line": "填补凡尘缺失的心跳"},
  {"time": "01:20.87", "line": "落于人间一笑 勾画醉意袅袅"},
  {"time": "01:27.25", "line": "似梦绕 画里画外扫寂寥"},
  {"time": "01:32.39", "line": "你是不食烟火的仙瑶"},
  {"time": "01:38.06", "line": "舞凤翔鸾相交的窈窕"},
  {"time": "01:43.97", "line": "题画诗也难描 千古词不敢扰"},
  {"time": "01:50.38", "line": "华灯照 墨香里印色正妙"},
